<odoo>
    <data>
         <record forcecreate="True" id="ir_cron_iti_student" model="ir.cron">
            <field name="name">Attendences: Email Queue Manager</field>
            <field name="model_id" ref="model_iti_student"/>
            <field name="state">code</field>
            <field name="code">model.process_email_queue()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall"/>
        </record>
    </data>
</odoo>